<robot name="niitblue_turtle">
  <link name="base_link">
    <origin xyz="0 0 0.076" rpy="0 0 0"/>
    <visual>
      <geometry>
        <cylinder length="0.142" radius="0.064" />
      </geometry>
      <material name="green">
        <color rgba="0.0 0.75 0.0 2.0"/>
      </material>
    </visual>
  </link>

  <joint name="base_link_to_WHEEL_L" type="continuous">
    <origin xyz="0.033 0.08 -0.048" rpy="1.57 0 0"/>
    <parent link="base_link"/>
    <child link="WHEEL_L"/>
  </joint>

  <link name="WHEEL_L">
    <visual>
      <geometry>
        <cylinder length="0.018" radius="0.033" />
      </geometry>
      <material name="dark_gray">
        <color rgba="0.25 0.25 0.25 2.0"/>
      </material>
    </visual>
  </link>

  <joint name="base_link_to_WHEEL_R" type="continuous">
    <origin xyz="0.033 -0.08 -0.048" rpy="-1.57 0 0"/>
    <parent link="base_link"/>
    <child link="WHEEL_R"/>
  </joint>

  <link name="WHEEL_R">
    <visual>
      <geometry>
        <cylinder length="0.018" radius="0.033" />
      </geometry>
      <material name="dark_gray">
        <color rgba="0.25 0.25 0.25 2.0"/>
      </material>
    </visual>
  </link>

  <joint name="base_link_to_CASTER" type="fixed">
    <origin xyz="-0.05 0 -0.076" rpy="0 0 0"/>
    <parent link="base_link"/>
    <child link="CASTER"/>
  </joint>

  <link name="CASTER">
    <visual>
      <geometry>
        <sphere radius="0.005" />
      </geometry>
      <material name="dark_gray">
        <color rgba="0.25 0.25 0.25 2.0"/>
      </material>
    </visual>
  </link>

</robot>
